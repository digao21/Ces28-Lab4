		super("");
	}	

	@Override
	public double getPrecoPorHora() {
		return 0;
	}

	@Override
	public double getPrecoPorUnidade() {
		return 0;
	}

	@Override
	public String getInformacao(String tipoInformacao) {
		return "Nao possuo tal informacao";
	}

}
